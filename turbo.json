{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "inputs": ["$TURBO_DEFAULT$", ".env"],
      "cache": false,
      "persistent": true
    },
    "web#db:push": {
      "inputs": ["$TURBO_DEFAULT$", ".env"]
    },
    "web#db:generate": {
      "inputs": ["$TURBO_DEFAULT$", ".env"]
    },
    "web#db:migrate": {
      "inputs": ["$TURBO_DEFAULT$", ".env"]
    },
    "web#db:drop": {
      "inputs": ["$TURBO_DEFAULT$", ".env"]
    },
    "web#db:studio": {
      "inputs": ["$TURBO_DEFAULT$", ".env"]
    }
  }
}
